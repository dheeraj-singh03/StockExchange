{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "Jwt": {
    "Issuer": "StockExchangeApi",
    "ExpiryMinutes": 3600,
    "Key": "SuperStrongSecurityKeyForJwtTokens123456"
  },
  "AllowedHosts": "*"
}
